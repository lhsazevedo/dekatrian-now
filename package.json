{
  "name": "dekatrianow",
  "description": "A simple page that displays the current date in the Dekatrian calendar.",
  "version": "0.1.0",
  "author": "Lucas Henrique Azevedo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --content-base src",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules && cp src/index.html dist/index.html",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "vitorteccom-dekajs": "^2.0.1",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "gh-pages": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
